    </servlet>
    <servlet>
        <servlet-name>rent</servlet-name>
        <servlet-class>servlet.rent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>h</servlet-name>
        <servlet-class>servlet.h</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>feed</servlet-name>
        <servlet-class>servlet.feed</servlet-class>
