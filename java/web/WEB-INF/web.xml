        <servlet-name>rent</servlet-name>
        <url-pattern>/rent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>h</servlet-name>
        <url-pattern>/h</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>feed</servlet-name>
        <url-pattern>/feed</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
